{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.0.1",
    "useDocker": false,
    "generators": {
      "python-client": {
        "packageName": "gwm_openapi_client",
        "generatorName": "python",
        "output": "client",
        "inputSpec": "http://localhost:8000/api/schema/",
        "globalProperty": "apiTests=false,modelTests=false",
        "additionalProperties": {
          "floatStrictType": false
        },
        "skipValidateSpec": true,
        "minimalUpdate": true
      }
    }
  }
}
